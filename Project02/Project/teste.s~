@.formatting.string = private constant [4 x i8] c"%d\0A\00"
%type.Array = type {i32, i32* }
define i32 @main() {
entry:
  %tmp0 = alloca i32
  store i32 0, i32 * %tmp0
  %tmp1 = mul i32 4, 1
  %tmp2 = call i8*  @malloc(i32 %tmp1)
  %tmp3 = bitcast i8* %tmp2 to %class.a* 
  %tmp4 = call i32  @_i__a(%class.a*  %tmp3)
  %tmp5 = getelementptr [4 x i8] * @.formatting.string, i32 0, i32 0
  %tmp6 = call i32 (i8 *, ...)* @printf(i8 * %tmp5, i32 %tmp4)
  %tmp7 = load i32 * %tmp0
  ret i32 %tmp7
}
%class.a = type {i32}
define i32 @_i__a(%class.a*  %this) {
entry_i__a:
  %tmp8 = getelementptr %class.a*  %this, i32 0, i32 0
  %tmp9 = load i32 * %tmp8
  store i32 10, i32 %tmp9
  %tmp10 = getelementptr %class.a*  %this, i32 0, i32 0
  %tmp11 = load i32 * %tmp10
  ret i32 %tmp11
}
declare i32 @printf (i8 *, ...)
declare i8 * @malloc (i32)
